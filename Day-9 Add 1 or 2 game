#include <iostream>
#include <vector>

using namespace std;

string winner(int N) {
    if (N % 3 == 0) {
        return "BOB";
    } else if (N % 3 == 1) {
        return "ALICE";
    } else {
        return "ALICE";
    }
}

int main() {
    int T;
    cin >> T;
    while (T--) {
        int N;
        cin >> N;
        cout << winner(N) << endl;
    }
    return 0;
}
